{
	"name": "dnd-spell-organizer",
	"private": false,
	"license": "MIT",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prettier": "prettier --config ./.prettierrc --ignore-path  ./.prettierignore --write \"**/*.ts*\" \"**/*.css\" \"*.json\" \"*.md\"",
		"prettier-check": "prettier --list-different \"{*.js, *.md,./{src}/**/*.ts?(x)}\"",
		"lint-check": "yarn prettier-check && yarn eslint && yarn stylelint-check",
		"lint": "yarn prettier; yarn eslint; yarn stylelint",
		"eslint": "eslint '*/**/*.{js,ts,tsx}'",
		"stylelint": "stylelint --config ./.stylelintrc --fix \"src/**/*.css\" --color --cache",
		"stylelint-check": "stylelint --config ./.stylelintrc \"src/**/*.css\" --color --cache"
	},
	"dependencies": {
		"ag-grid-community": "^29.1.0",
		"ag-grid-react": "^29.1.0",
		"bootstrap": "^5.2.3",
		"react": "^18.2.0",
		"react-bootstrap": "^2.7.2",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"prettier": "^2.8.4",
		"stylelint": "^15.2.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-prettier": "^2.0.0",
		"typescript": "^4.9.3",
		"vite": "^4.1.0"
	}
}
